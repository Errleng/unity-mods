    <!--Assimilate Cravers Self9-->
    <StarSystemPopulationTransformImprovementDefinition   Name="StarSystemImprovementAssimilateCraversPopulationSelf9" SubCategory="SubCategoryPopulation" ScoreProvider="StarSystemImprovementBuilt"
                                                    PopulationSource="AffinityCravers"  PopulationDestination="Major" PopulationToTransformCount="1">
      <Tags>NoBuyout</Tags>
		  <Cost ResourceName="SystemTurnBased">1</Cost>
      <PathPrerequisite Inverted="false" Flags="Prerequisite,Discard">ColonizedStarSystemStateColony</PathPrerequisite>
      <QueuedSimulationDescriptorReference Name="StarSystemImprovementCraversPopulationQueued"/>
      <InterpreterPrerequisite Inverted="false" Flags="NotEnoughPopulation,Enqueue">(SumProperty(Context, @'../ClassColonizedStarSystem/ClassPopulationStarSystemAffinityCraversCount,ClassPopulation', PopulationCount) - Property(Context, @ClassColonizedStarSystem, OnGoingCravers) ) gt 0</InterpreterPrerequisite>
      <PathPrerequisite Inverted="false"   Flags="Prerequisite,Discard">../ClassEmpire,AffinityGameplayCravers</PathPrerequisite>
      <InterpreterPrerequisite Flags="Prerequisite,Discard">SumProperty(Context, @'../ClassEmpire/ClassPopulation,ClassPopulationEmpireAffinityCustom9Count', PopulationCount) ge 1</InterpreterPrerequisite> 
      <InterpreterPrerequisite Flags="Prerequisite,Discard">SumProperty(Context, @'../ClassColonizedStarSystem/ClassPopulationStarSystemAffinityCraversCount,ClassPopulation', PopulationCount) ge 1</InterpreterPrerequisite>
      <SimulationDescriptorReference Name="StarSystemImprovementAssimilateCraversPopulationSelf9"/>
